{
  "manifest_version": 2,
  "name": "Hacked Emails checker for GMail",
  "version": "1.5.2",
  "description": "Hacked-Emails.com checker for GMail. Anonymously check if an email sender has ever been compromised in a security breach",
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*"
      ],
      "js": [
        "/js/content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "/js/lib/jquery.js",
    "/js/lib/gmail.js",
    "/js/main.js",
    "/css/styles.css"
  ],
  "icons": {
    "16": "/icons/icon16.png",
    "48": "/icons/icon48.png",
    "128": "/icons/icon128.png"
  }
}